{
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowJs": true,
    "target": "es5",
    "outDir": ".build",
    "moduleResolution": "node",
    "lib": ["es2015", "es2020.string"],
    "downlevelIteration": true,
    "rootDir": "./",
    "resolveJsonModule":true,
    "esModuleInterop": true
  }
}
